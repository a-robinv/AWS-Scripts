aws application-autoscaling put-scheduled-action --service-namespace rds --scheduled-action-name haku-rds-push-scaleout-1030AM --schedule "cron(30 2 * * ? *)" --resource-id cluster:haku-prod2-rds-01-instance --scalable-dimension rds:cluster:ReadReplicaCount --scalable-target-action MinCapacity=5

aws application-autoscaling put-scheduled-action --service-namespace rds --scheduled-action-name haku-rds-push-scalein-1135AM --schedule "cron(15 3 * * ? *)" --resource-id cluster:haku-prod2-rds-01-instance --scalable-dimension rds:cluster:ReadReplicaCount --scalable-target-action MinCapacity=1